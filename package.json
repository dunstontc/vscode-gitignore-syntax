{
    "name": "vscode-gitignore-syntax",
    "displayName": "vscode-gitignore-syntax",
    "description": "Language support for .gitignore files.",
    "version": "0.0.1",
    "publisher": "dunstontc",
    "engines": {
        "vscode": "^1.20.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "gitignore",
            "aliases": ["gitignore", "gitignore"],
            "extensions": ["gitignore"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "gitignore",
            "scopeName": "gitignore",
            "path": "./syntaxes/gitignore.tmLanguage.json"
        }]
    }
}
