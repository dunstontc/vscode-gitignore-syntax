{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "gitignore",
	"patterns": [
		{
			"include": "#keywords"
		},
		{
			"include": "#comments"
		}
	],
	"repository": {
		"keywords": {
			"patterns": [{
				"name": "keyword.control.glob",
				"match": "(\\*)"
			}]
		},
		"comments": {
			"name": "comment.line.gitignore",
			"begin": "# ",
			"end": "$",
			"patterns": [
			]
		}
	},
	"scopeName": "source.gitignore"
}
